Query GetTodosQuery {}

QueryHandler GetTodosQueryHandler (todoRepo: TodoReadRepoPort) {
    execute (query: GetTodosQuery): (OK(TodoReadModel[]), Errors()) {
      const todos = this.todoRepo.getAll();
      if(todos is Error){
        return Error(todos);
      }
  
      if (NOT todos) {
        return [];
      }
      return todos;
    }
}